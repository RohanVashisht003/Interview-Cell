        <a href=" " data-mdb-toggle="tooltip" title="Edit Details"><i class="fas fa-edit fa-lg text-success mr-2"></i></a>
        <a href="" data-mdb-toggle="tooltip" title="Delete"><i
        class="fas fa-trash-alt fa-lg text-warning"></i></a>
